{
    "extends": "airbnb-base/legacy",
    "env": {
        "es6": true,
        "node": true
    },

    "rules": {
        "indent": 0,
        "strict": 0,
        "no-var": "error",
        "no-console": 0,
        "no-unused-vars": "off",
        "comma-dangle": "off"
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    }
}
